{
  "name": "scrapient",
  "version": "0.3.0",
  "description": "Simplified extension-first web scraping tool",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --outdir dist",
    "build:extensions": "bun extensions/scripts/build-chrome.ts && bun extensions/scripts/build-firefox.ts",
    "watch:extensions": "bun extensions/scripts/watch.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "bun scripts/setup.ts",
    "llm:download": "bun scripts/download-model.ts",
    "llm:test": "bun scripts/test-llm.ts",
    "test": "echo 'No tests yet' && exit 0"
  },
  "keywords": ["web-scraping", "browser-extension", "content-extraction", "typescript"],
  "author": "scrapient",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "mongodb": "^7.0.0",
    "node-llama-cpp": "^3.14.2",
    "zod": "^3.23.8"
  }
}